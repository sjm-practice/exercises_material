{
  "verbose": false,
  "clearMocks": true,
  "setupTestFrameworkScriptFile": "<rootDir>/tests/jestSetup.js",
  "testURL": "http://localhost:3000",
  "snapshotSerializers": [
    "enzyme-to-json/serializer"
  ],
  "moduleNameMapper": {
    "^meteor/(.*)$": "<rootDir>/tests/jest-meteor-mocks/$1.js"
  },
  "testPathIgnorePatterns": [
    "/node_modules/",
    "/.meteor/",
    "/public/"
  ],
  "testRegex": "/tests/.*\\.jest\\.jsx?$"
}
